{
  "name": "msfish",
  "version": "1.0.0",
  "author": "Darrell Dupas",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddupas/msfish.git"
  },
  "main": "index.js",
  "dependencies": {
    "discord.js": "^14.15.3",
    "http-server": "^14.1.1",
    "jsdom": "^24.1.1",
    "node-emoji": "^2.1.3",
    "node-schedule": "^2.1.1",
    "sqlite3": "^5.1.7"
  },
  "bugs": {
    "url": "https://github.com/ddupas/msfish/issues"
  },
  "description": "msfish",
  "homepage": "https://github.com/ddupas/msfish#readme",
  "imports": {
    "#events/*": "./discord-bot/events/*",
    "#commands/*": "./discord-bot/events/commands/*.mjs"
  },
  "keywords": [
    "node",
    "discord.js",
    "sqlite"
  ],
  "license": "ISC",
  "scripts": {
    "deploy-commands": "cd discord-bot; bun deploy-commands.mjs",
    "discord-bot": "bun --expose-gc discord-bot/manage-bot.mjs",
    "test-deletedup": "bun tests/test-deletedup.mjs -n",
    "test-gitpushdb": "bun tests/test-gitpushdb.mjs",
    "test-next": "bun tests/test-next.mjs",
    "test-snapshot": "bun test-snapshot.js",
    "test-updateplayers": "bun tests/test-updateplayers.mjs -n",
    "msfish": "bun msfish.mjs",
    "pm2-nodaemon": "pm2 --no-daemon start pm2/ecosystem.config.cjs",
    "http-server": "http-server -o -c-1 -p 80",
    "bun-update": "bun update --save",
    "renderdotcomx": "pm2 --no-daemon start pm2/ecosystem.config.cjs",
    "renderwithpm2": ". ./pm2.link.secret.sh && pm2 link $PM2LINKSECRET && pm2 --no-daemon start pm2/ecosystem.config.cjs",
    "renderdotcom": "bun run deploy-commands && bun run discord-bot & bun run msfish & bun run http-server"
  },
  "type": "module"
}
